{
    "manifest_version": 3,
    "name": "Tab Saver",
    "description": "Save and open tabs automatically",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "index.js"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}



